{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": [0],
        "linebreak-style": 0,
        "prefer-destructuring": 0,
        "react/destructuring-assignment": [0],
        "jsx-a11y/media-has-caption": [0],
        "max-len": [2, 150]
    }
}
